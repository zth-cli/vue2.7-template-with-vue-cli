{
  "name": "vue-template-with-elementui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:auto": "node autoDeploy.js",
    "build:dev": "node autoDeploy.js dev",
    "build:pro": "node autoDeploy.js pro"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.24.1",
    "dayjs": "^1.8.17",
    "echarts": "^4.9.0",
    "element-ui": "^2.4.5",
    "js-cookie": "^2.2.1",
    "nprogress": "^0.2.0",
    "vue": "^2.7.0",
    "vue-particles": "^1.0.9",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.6",
    "@vue/cli-plugin-eslint": "~5.0.6",
    "@vue/cli-plugin-router": "~5.0.6",
    "@vue/cli-plugin-vuex": "~5.0.6",
    "@vue/cli-service": "~5.0.6",
    "@vue/eslint-config-standard": "^5.1.2",
    "archiver": "^4.0.1",
    "babel-eslint": "^7.2.3",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^9.3.0",
    "sass": "^1.54.4",
    "sass-loader": "^8.0.2",
    "ssh2": "^0.8.9",
    "unplugin-auto-import": "^0.11.1",
    "unplugin-vue-components": "^0.22.4",
    "vue-cli-plugin-element": "~1.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "space-before-function-paren": [
        "error",
        {
          "anonymous": "always",
          "named": "never",
          "asyncArrow": "always"
        }
      ]
    },
    "no-eval": "error"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
